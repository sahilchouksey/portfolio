{
  "name": "osmic-portfolio-clone",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A pixel-perfect clone of the Osmic portfolio website built with React + Vite",
  "keywords": [
    "react",
    "vite",
    "portfolio",
    "webflow",
    "clone",
    "responsive"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:resume && vite build",
    "build:resume": "typst compile src/resume/resume.typ public/resume.pdf",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "motion": "^12.23.22",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
